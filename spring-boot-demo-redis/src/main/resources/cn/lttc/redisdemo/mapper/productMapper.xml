<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.lttc.redisdemo.mapper.ProductMapper">
    <select id="findProductById" resultType="cn.lttc.redisdemo.model.Product">
        select id,name,price from t_demo_redis_product where id = #{id}
    </select>
    <select id="findAllProduct" resultType="cn.lttc.redisdemo.model.Product">
        select id,name,price from t_demo_redis_product
    </select>
</mapper>